[@@@program]
[@@@import "../src-types/model_tags.iml"]
open Model_tags;;


let tag_to_message (x)  =
    (match x with
        | "D" -> (Some M_Msg_NewOrderSingle_Tag)
        | "8" -> (Some M_Msg_ExecutionReport_Tag)
        | _ -> None
    )
;;

let message_to_tag (x)  =
    (match x with
        | M_Msg_NewOrderSingle_Tag -> "NewOrderSingle"
        | M_Msg_ExecutionReport_Tag -> "ExecutionReport"
    )
;;

let tag_to_rg (x)  =
    (match x with
        | "PtysSubGrp" -> (Some M_RG_PtysSubGrp_Tag)
        | "Parties" -> (Some M_RG_Parties_Tag)
        | _ -> None
    )
;;

let tag_to_field (x)  =
    (match x with
        | "54" -> (Some M_Field_Side_Tag)
        | "60" -> (Some M_Field_TransactTime_Tag)
        | "40" -> (Some M_Field_OrdType_Tag)
        | "38" -> (Some M_Field_OrderQty_Tag)
        | "18" -> (Some M_Field_ExecInst_Tag)
        | "448" -> (Some M_Field_PartyID_Tag)
        | "10002" -> (Some M_Field_PartyIndex_Tag)
        | "37" -> (Some M_Field_OrderID_Tag)
        | "17" -> (Some M_Field_ExecID_Tag)
        | "150" -> (Some M_Field_ExecType_Tag)
        | "39" -> (Some M_Field_OrdStatus_Tag)
        | "151" -> (Some M_Field_LeavesQty_Tag)
        | "14" -> (Some M_Field_CumQty_Tag)
        | _ -> None
    )
;;

let field_to_tag (x)  =
    (match x with
        | M_Field_Side_Tag -> "Side"
        | M_Field_TransactTime_Tag -> "TransactTime"
        | M_Field_OrdType_Tag -> "OrdType"
        | M_Field_OrderQty_Tag -> "OrderQty"
        | M_Field_ExecInst_Tag -> "ExecInst"
        | M_Field_PartyID_Tag -> "PartyID"
        | M_Field_PartyIndex_Tag -> "PartyIndex"
        | M_Field_OrderID_Tag -> "OrderID"
        | M_Field_ExecID_Tag -> "ExecID"
        | M_Field_ExecType_Tag -> "ExecType"
        | M_Field_OrdStatus_Tag -> "OrdStatus"
        | M_Field_LeavesQty_Tag -> "LeavesQty"
        | M_Field_CumQty_Tag -> "CumQty"
    )
;;

[@@@logic]
