let check_field_mod_f_NewOrderSingle_SpreadProportion_inner0 (msg_data : mod_newordersingle_data) (_local_internal_state : fix_internal_state) (_local_internal_fresh_state' : fix_internal_fresh_state')  =
    (if ((match msg_data.mod_f_NewOrderSingle_SpreadProportion with
        | Some cap_x -> ((&&) (float_GreaterThan_6_6 cap_x (float_Create_6 (0))) (float_LessThanEqual_6_6 cap_x (float_Create_6 (1000000))))
        | None -> true
    )
    ) then
        fields_good_defrej
    else
        (is_fields_invalid_defrej {
            mod_builtin_rejectInfo_text = {l|Field SpreadProportion of Message NewOrderSingle violates the constraint: " case(it){None:true}{Some x: x>0.0 &&  x<=1.0}"|l};
            mod_builtin_rejectInfo_field = Some {l|SpreadProportion|l}
        }))
;;

let check_field_mod_f_NewOrderSingle_SpreadProportion_outer (msg_data : mod_newordersingle_data) (local_internal_state : fix_internal_state) (local_internal_fresh_state' : fix_internal_fresh_state')  =
