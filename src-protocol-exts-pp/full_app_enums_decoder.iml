let full_enum_execinst_decoder  : fix_execinst list decoder =
    list (string >>= (fun x -> 
    (match string_to_execinst_opt x with
        | None -> (fail "Unknown enum (ExecInst) encoding.")
        | Some y -> (succeed y)
    )
    ))
;;

