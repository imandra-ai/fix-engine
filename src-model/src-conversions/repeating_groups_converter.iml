    let d = (match rec_in.rg_Parties_element_0 with
        | None -> d
        | Some x -> (x :: d)
    )
    in
    let d = (match rec_in.rg_Parties_element_1 with
        | None -> d
        | Some x -> (x :: d)
    )
    in
    let d = (match rec_in.rg_Parties_element_2 with
        | None -> d
